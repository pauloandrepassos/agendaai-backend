{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                "**/*"
                ]
            }
        } 
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts",
			"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
			"headers": {"Access-Control-Allow-Origin": "*"}
        }
    ]
}
  